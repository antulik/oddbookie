<% if params[:jquery_version] && !params[:jquery_version].empty? %>
<script type='text/javascript' src="//ajax.googleapis.com/ajax/libs/jquery/<%= params[:jquery_version] %>/jquery.min.js"></script>
<% end %>

<script type="text/javascript">
    var _osq = _osq || [];
    _osq.push(['setAccount', '<%= params[:oddsense_account] %>']);

    function _oss(u){
        setTimeout(function(){
            var d = document, f = d.getElementsByTagName('script')[0],
                    s = d.createElement('script');
            s.type = 'text/javascript'; s.async = true; s.src = u;
            f.parentNode.insertBefore(s, f);
        }, 1);
    }
    _oss('<%= params[:oddsense_js] %>');
</script>

<script type='text/javascript'>
<%= params[:js_code] %>
</script>
